<?xml version="1.0"?>

<robot name="cube">

<!-- Links -->
	<link name="cube_base_link">
		<visual>
      			<geometry>
				<mesh filename="package://mobil/meshes/40x40CubeModelCenterOnOrigin.dae"/>
			</geometry>
		</visual>
		<collision>
      			<geometry>
        			<box size="1.0 1.0 0.001"/>
      			</geometry>
    		</collision>
    		<inertial>
	      		<mass value="10"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>
	</link>

	<link name="cube_4x4_1_link">
    		<visual>
      			<geometry>
         			<box size="0.001 0.001 0.001"/>
      			</geometry>
      			<origin rpy="0 0 0" xyz="0 0 0"/>
    		</visual>
    		<inertial>
      			<mass value="0.0001"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>	
	</link>

	<link name="cube_4x4_2_link">
    		<visual>
      			<geometry>
         			<box size="0.001 0.001 0.001"/>
      			</geometry>
      			<origin rpy="0 0 0" xyz="0 0 0"/>
    		</visual>
    		<inertial>
      			<mass value="0.0001"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>	
	</link>
	
	<link name="cube_4x4_3_link">
    		<visual>
      			<geometry>
         			<box size="0.001 0.001 0.001"/>
      			</geometry>
      			<origin rpy="0 0 0" xyz="0 0 0"/>
    		</visual>
    		<inertial>
      			<mass value="0.0001"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>	
	</link>
	
		<link name="cube_4x4_4_link">
    		<visual>
      			<geometry>
         			<box size="0.001 0.001 0.001"/>
      			</geometry>
      			<origin rpy="0 0 0" xyz="0 0 0"/>
    		</visual>
    		<inertial>
      			<mass value="0.0001"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>	
	</link>
	
	<link name="cube_4x4_5_link">
    		<visual>
      			<geometry>
         			<box size="0.001 0.001 0.001"/>
      			</geometry>
      			<origin rpy="0 0 0" xyz="0 0 0"/>
    		</visual>
    		<inertial>
      			<mass value="0.0001"/>
      			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    		</inertial>	
	</link>

<!-- Joints -->
	<joint name="cube_4x4_1_joint" type="fixed">
		<origin xyz="0 -0.5 0.5" />
    	<parent link="cube_base_link" />
    	<child link="cube_4x4_1_link" />
	</joint>

	<joint name="cube_4x4_2_joint" type="fixed">
		<origin xyz="-0.5 0 0.5" />
    	<parent link="cube_base_link" />
    	<child link="cube_4x4_2_link" />
	</joint>

	<joint name="cube_4x4_3_joint" type="fixed">
		<origin xyz="0 0.5 0.5" />
    	<parent link="cube_base_link" />
    	<child link="cube_4x4_3_link" />
	</joint>

	<joint name="cube_4x4_4_joint" type="fixed">
		<origin xyz="0.5 0 0.5" />
    	<parent link="cube_base_link" />
    	<child link="cube_4x4_4_link" />
	</joint>
	
	<joint name="cube_4x4_5_joint" type="fixed">
		<origin xyz="0 0 1" />
    	<parent link="cube_base_link" />
    	<child link="cube_4x4_5_link" />
	</joint>

<!-- Gazebo -->
	<gazebo reference="base_footprint_1">
    		<material>Gazebo/Blue</material>
	</gazebo>

  <gazebo>
    <plugin name="object_controller" filename="libgazebo_ros_planar_move.so">
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <odometryRate>20.0</odometryRate>
      <frameName>world</frameName>
      <robotBaseFrame>cube_base_link</robotBaseFrame>
    </plugin>
  </gazebo>

</robot> <!--end robot "cube -->
